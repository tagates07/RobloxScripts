local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "[Plants vs Zombies Defenders] [V0.75.34(Beta)] (Gui Hub)", HidePremium = false, SaveConfig = false, ConfigFolder = "Config [[Plants vs Zombies Defenders] (Gui Hub)]]"})

OrionLib:MakeNotification({
	Name = "Script Loaded",
	Content = "This script is made by Doge The Doggo#3390",
	Image = "rbxassetid://4483345998",
	Time = 10
})

local Tab = Window:MakeTab({
	Name = "Lobby",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Unequip all items",
	Callback = function()
		local args = {
			[1] = 6,
			[2] = game:GetService("Players").LocalPlayer
		}
		
		game:GetService("ReplicatedStorage").Plants:FindFirstChild("UnequipPlanta \195\145"):FireServer(unpack(args))
		local args = {
			[1] = 5,
			[2] = game:GetService("Players").LocalPlayer
		}
		
		game:GetService("ReplicatedStorage").Plants:FindFirstChild("UnequipPlanta \195\145"):FireServer(unpack(args))
		local args = {
			[1] = 4,
			[2] = game:GetService("Players").LocalPlayer
		}
		
		game:GetService("ReplicatedStorage").Plants:FindFirstChild("UnequipPlanta \195\145"):FireServer(unpack(args))
		local args = {
			[1] = 3,
			[2] = game:GetService("Players").LocalPlayer
		}
		
		game:GetService("ReplicatedStorage").Plants:FindFirstChild("UnequipPlanta \195\145"):FireServer(unpack(args))
		local args = {
			[1] = 2,
			[2] = game:GetService("Players").LocalPlayer
		}
		
		game:GetService("ReplicatedStorage").Plants:FindFirstChild("UnequipPlanta \195\145"):FireServer(unpack(args))
		local args = {
			[1] = 1,
			[2] = game:GetService("Players").LocalPlayer
		}
		
		game:GetService("ReplicatedStorage").Plants:FindFirstChild("UnequipPlanta \195\145"):FireServer(unpack(args))
		
  	end    
})

